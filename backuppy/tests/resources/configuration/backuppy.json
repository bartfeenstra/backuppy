{
  "name": "Test",
  "verbose": true,
  "interactive": false,
  "exclude": ["./source/cache", "./source/tmp/*"],
  "include": ["./source/tmp/backup_this_after_all"],
  "notifications": [
    {
      "type": "stdio"
    }
  ],
  "source": {
    "type": "path",
    "configuration": {
      "path": "./source"
    }
  },
  "target": {
    "type": "first_available",
    "configuration": {
      "targets": [
        {
          "type": "ssh",
          "configuration": {
            "host": "example.com",
            "port": 22,
            "user": "bart",
            "path": "./target"
          }
        }
      ]
    }
  },
  "logging": {
    "version": 1
  }
}
